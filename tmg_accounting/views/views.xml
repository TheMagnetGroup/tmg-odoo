<odoo>
  <data>
    <record id="view_company_form_remit_to_inherit" model="ir.ui.view">
          <field name="name">res.company.form.remit.to.inherit</field>
          <field name="model">res.company</field>
          <field name="inherit_id" ref="base.view_company_form" />
          <field name="arch" type="xml">
             <xpath expr="//field[@name='nomenclature_id']" position="after">
                <field name="remit_to_id"/>
             </xpath>
      </field>
    </record>
  </data>
</odoo>