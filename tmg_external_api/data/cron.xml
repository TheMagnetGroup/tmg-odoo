<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="tmg_ir_cron_check_sla" model="ir.cron">
        <field name="name">Reset API Call Count</field>
        <field name="model_id" ref="model_res_partner"/>
        <field name="state">code</field>
        <field name="code">model.reset_call_cap()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
    </record>

    <record id="tmg_ir_cron_sage_product_inactivation" model="ir.cron">
        <field name="name">Castelli Export Account: Sage Product Inactivation</field>
        <field name="model_id" ref="model_tmg_external_api_tmg_export_account"/>
        <field name="state">code</field>
        <field name="code">model.check_sage_castelli_products()</field>
        <field name="interval_number">24</field>
        <field name="interval_type">hours</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
    </record>

</odoo>
